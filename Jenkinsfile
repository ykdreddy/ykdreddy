pipeline {
    agent any

    stages {
        stage("SCM Clone repo") {
            steps {
                echo "clone repo"
            }
        }

        stage "Build" {
            steps {
                echo "Build"
            }
        }

        stage "test" {
            steps {
                echo "test"
            }
        }

        stage "Deploy the repo" {
            steps {
                echo "deploy"
            }
        }
    }
}

