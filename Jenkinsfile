pipeline {
    agent any

    stages {
        stage("SCM Clone repo") {
            steps {
                echo "clone repo"
            }
        }

        stage("Build") {
            steps {
                echo "Build"
            }
        }

        stage("Test") {
            steps {
                echo "Test"
            }
        }

        stage("Deploy the repo") {
            steps {
                echo "Deploy"
            }
        }
    }
}
